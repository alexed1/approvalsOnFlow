    <decisions>
        <name>Action_Taken</name>
        <label>Action Taken?</label>
        <locationX>317</locationX>
        <locationY>326</locationY>
        <defaultConnectorLabel>Recall</defaultConnectorLabel>
        <rules>
            <name>Approve</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TakenAction</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approve</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Which_Decisioning_Process</targetReference>
            </connector>
            <label>Approve</label>
        </rules>
        <rules>
            <name>Reject</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TakenAction</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Reject</stringValue>
                </rightValue>
            </conditions>
            <label>Reject</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_there_a_Next_Step</name>
        <label>Is there a Next Step?</label>
        <locationX>1136</locationX>
        <locationY>332</locationY>
        <defaultConnector>
            <targetReference>Run_Final_Approval_Flow</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>nextStepDefinition</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Process_Instance_Current_Step</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Which_Decisioning_Process</name>
        <label>Which Decisioning Process?</label>
        <locationX>504</locationX>
        <locationY>326</locationY>
        <defaultConnectorLabel>Majority rules</defaultConnectorLabel>
        <rules>
            <name>Use_First_Received_Response</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>curApprovalProcessStepDefinition.DecisionCriteria__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>OneApprovalNeeded</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Run_Approval_Flow</targetReference>
            </connector>
            <label>Use First Received Response</label>
        </rules>
        <rules>
            <name>Must_be_unanimous</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>curApprovalProcessStepDefinition.DecisionCriteria__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>UnanimousNeeded</stringValue>
                </rightValue>
            </conditions>
            <label>Must be unanimous</label>
        </rules>
    </decisions>
